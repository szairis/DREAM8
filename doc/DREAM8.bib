month = sep,
number = {38},
pages = {14313--8},
pmid = {18799736},
title = {{Network inference using informative priors.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2567188\&tool=pmcentrez\&rendertype=abstract},
volume = {105},
year = {2008}
