issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
keywords = {Breast Neoplasms,Breast Neoplasms: enzymology,Cell Line, Tumor,Female,Gene Regulatory Networks,Humans,Kinetics,MAP Kinase Signaling System,Markov Chains,Monte Carlo Method,Phosphorylation,Proteomics,Systems Biology,Systems Biology: methods},
